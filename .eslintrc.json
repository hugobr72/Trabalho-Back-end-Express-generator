{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "object-curly-spacing": 0,
        "new-cap": 0,
        "space-before-function-paren": 0,
        
        "indent": [ "error", 2]
    }
}
